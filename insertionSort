def insertionSort1(n, arr):
    for i in range(1,n):
        j = i
        val = arr[i]
        while val<arr[i-1] and i>0:
            arr[i] = arr[i-1]
            i -= 1
            for index in range(n):
                print(arr[index], end=' ')
            print()
        arr[i] = val
        if i!=j:
            for index in range(n):
                print(arr[index], end=' ')
